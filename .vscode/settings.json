{
    "files.exclude": {
        "**/*.aux": true,
        "**/*.blg": true,
        "**/*.bcf": true,
        "**/*.fls": true,
        "**/*.lof": true,
        "**/*.out": true,
        "**/*.toc": true,
        "**/*.lot": true,
        "**/*.log": true,
        "**/*.fdb_latexmk": true,
        "**/*.run.xml": true,
        "**/*.synctex.gz": true,
        "**/*.bbl": true,
        "**/*-blx.bib": true
      },
    "rewrap.autoWrap.enabled": true,
    "rewrap.autoWrap.notification": "icon",
    "rewrap.wrappingColumn": 90,
    "grammarly.selectors": [
      {
        "language": "latex",
        "scheme": "file"
      }
    ]
}